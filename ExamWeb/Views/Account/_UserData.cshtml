
@{
    ViewBag.Title = "_UserData";
}

<div class="container__form-bloque">
    <div class="container__form-bloque-titulo">
        <h2>Usuario</h2>
        @if (@Model != null)
        {
            string dato = Convert.ToString(@Model);
            @Html.ActionLink(dato, "Registrar", "Account")
        }
    </div>
    <input type="text" name="user" value="" />
</div>
<div class="container__form-bloque">
    <h2>Contraseña</h2>
    <input type="password" name="pass" value="" />
</div>